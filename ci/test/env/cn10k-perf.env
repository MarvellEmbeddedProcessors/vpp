source $PROJECT_ROOT/ci/test/env/cn10k.env

# List of perf tests to be run.
RUN_TESTS="
	l3fwd_perf
	inl_ipsec_perf
"

# Update command timeout
CMD_TIMEOUTS="l3fwd_perf=10m $CMD_TIMEOUTS"

# Perf stage flag
PERF_STAGE=1

# Continue testing regardless of failure
CONTINUE_ON_FAILURE=1

# Export the path to this conf so that other scripts can source this conf.
export TEST_ENV_CONF=$PROJECT_ROOT/ci/test/env/cn10k-perf.env
